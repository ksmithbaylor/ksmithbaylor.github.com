{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "main": "index.js",
  "scripts": {
    "dev:hugo": "hugo -s site --buildDrafts --buildFuture server",
    "dev:webpack": "NODE_ENV=development webpack -w --display-optimization-bailout",
    "start": "run-p -l dev:*",
    "build:webpack": "NODE_ENV=production webpack",
    "build:hugo": "hugo -d ../dist -s site",
    "build": "run-p build:*",
    "preview:hugo": "hugo -d ../dist -s site -b http://localhost:8080",
    "preview:serve": "http-server -p 8080 dist",
    "preview": "run-s preview:hugo build:webpack preview:serve",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.11.6",
    "css-loader": "^0.28.2",
    "eslint": "^3.1.1",
    "eslint-plugin-import": "^1.11.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.11.1",
    "http-server": "^0.10.0",
    "imports-loader": "^0.7.1",
    "npm-run-all": "^4.0.2",
    "promise-polyfill": "^6.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^3.0.0",
    "whatwg-fetch": "^1.0.0"
  }
}
