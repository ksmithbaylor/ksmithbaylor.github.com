{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "main": "index.js",
  "scripts": {
    "dev:hugo": "hugo -s site --buildDrafts --buildFuture server",
    "dev:rollup": "rollup -c -w",
    "start": "run-p -l dev:*",
    "build:rollup": "NODE_ENV=production rollup -c",
    "build:hugo": "hugo -d ../dist -s site",
    "build": "run-p build:*",
    "preview:hugo": "hugo -d ../dist -s site -b http://localhost:8080",
    "preview:serve": "http-server -p 8080 dist",
    "preview": "run-s preview:hugo build:rollup preview:serve",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.5.0",
    "http-server": "^0.10.0",
    "npm-run-all": "^4.0.2",
    "promise-polyfill": "^6.0.2",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
